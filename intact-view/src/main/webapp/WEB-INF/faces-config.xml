        <!--<managed-bean-class>uk.ac.ebi.intact.services.search.stats.StatusControllerd-bean-class>-->
        <!--configured in beans.spring.xml, declared here only for IDE purposes -->
        <!--<managed-bean-scope>none</managed-bean-scope>-->
    <!--</managed-bean>-->
  
</faces-config>