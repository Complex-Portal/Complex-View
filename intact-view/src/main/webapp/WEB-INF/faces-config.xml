<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

  <application>
      <default-render-kit-id>
          org.apache.myfaces.trinidad.core
      </default-render-kit-id>
      <el-resolver>
          org.springframework.web.jsf.el.SpringBeanFacesELResolver
      </el-resolver>
      <resource-bundle>
          <base-name>uk.ac.ebi.intact.Athena</base-name>
          <var>config</var>
      </resource-bundle>
  </application>




    <!-- Search and results -->

    <managed-bean>
        <managed-bean-name>searchBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.SearchController</managed-bean-class>
         <!--configured in beans.spring.xml, declared here only for IDE purposes -->
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>interactionResultsBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.InteractionResultsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>interactorResultsBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.InteractorResultsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>experimentResultsBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.ExperimentResultsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>cvObjectResultsBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.CvObjectResultsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <!-- Interactor views/controllers -->

     <managed-bean>
        <managed-bean-name>interactorBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.interactor.InteractorController</managed-bean-class>
        <!-- configured in beans.spring.xml, declared here only for IDE purposes -->
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>

    <!-- Interaction views/controllers -->

    <managed-bean>
        <managed-bean-name>interactionBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.interaction.InteractionController</managed-bean-class>
        <!-- configured in beans.spring.xml, declared here only for IDE purposes -->
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>

    <!-- Experiment views/controllers -->

    <managed-bean>
        <managed-bean-name>experimentBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.services.search.controller.experiment.ExperimentController</managed-bean-class>
        <!-- configured in beans.spring.xml, declared here only for IDE purposes -->
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>

    <!-- Binary search -->
     <!-- view controllers / backing beans -->
    <managed-bean>
        <managed-bean-name>appConfigBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.binarysearch.webapp.application.AppConfigBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>requestConfigBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.binarysearch.webapp.application.RequestConfigBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>olsBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.binarysearch.webapp.application.OlsBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>adminBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.binarysearch.webapp.view.AdminBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>config</property-name>
            <value>#{appConfigBean.config}</value>
        </managed-property>
        <managed-property>
            <property-name>configFileLocation</property-name>
            <value>#{appConfigBean.configFileLocation}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>firstTimeBean</managed-bean-name>
        <managed-bean-class>uk.ac.ebi.intact.binarysearch.webapp.view.FirstTimeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>config</property-name>
            <value>#{appConfigBean.config}</value>
        </managed-property>
    </managed-bean>

