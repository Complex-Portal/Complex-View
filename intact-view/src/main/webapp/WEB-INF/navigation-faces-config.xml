<?xml version="1.0"?>
<!-- In this file we define the navigation model for our application -->
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xi="http://www.w3.org/2001/XInclude"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <navigation-rule>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/search.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>interaction</from-outcome>
            <to-view-id>/pages/interaction/interaction.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>interactor</from-outcome>
            <to-view-id>/pages/interactor/interactor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>experiment</from-outcome>
            <to-view-id>/pages/experiment/experiment.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <!-- binary search -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>binarysearch</from-outcome>
            <to-view-id>/pages/binarysearch/binarysearch.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


</faces-config>