<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
    </application>

    <factory>
       <exception-handler-factory>uk.ac.ebi.intact.editor.application.EditorExceptionHandlerFactory</exception-handler-factory>
   </factory>

    <lifecycle>
        <!--<phase-listener>uk.ac.ebi.intact.editor.application.DebugPhaseListener</phase-listener>-->
        <phase-listener>uk.ac.ebi.intact.editor.application.HistoryPhaseListener</phase-listener>
        <phase-listener>uk.ac.ebi.intact.editor.application.LastActivityPhaseListener</phase-listener>
        <phase-listener>uk.ac.ebi.intact.editor.application.CacheControlPhaseListener</phase-listener>
    </lifecycle>

    <!--<component>-->
        <!--<component-type>uk.ac.ebi.intact.editor.InputBioSource</component-type>-->
        <!--<component-class>uk.ac.ebi.intact.editor.component.inputbiosource.InputBioSource</component-class>-->
    <!--</component>-->
    <!--<component>-->
        <!--<component-type>uk.ac.ebi.intact.editor.InputCvObject</component-type>-->
        <!--<component-class>uk.ac.ebi.intact.editor.component.inputcvobject.InputCvObject</component-class>-->
    <!--</component>-->

    <tag>
        <tag-name>ajax</tag-name>
        <behavior>
            <behavior-id>org.primefaces.component.AjaxBehavior</behavior-id>
            <handler-class>org.primefaces.component.behavior.ajax.AjaxBehaviorHandler</handler-class>
        </behavior>
		<attribute>
			<description></description>
			<name>listener</name>
			<required>false</required>
			<type>javax.el.MethodExpression</type>
		</attribute>
		<attribute>
			<description></description>
			<name>immediate</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description></description>
			<name>async</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description></description>
			<name>process</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>update</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>onstart</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>oncomplete</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>onerror</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>onsuccess</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description></description>
			<name>global</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
        <attribute>
			<description></description>
			<name>disabled</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
        <attribute>
			<description></description>
			<name>event</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
    </tag>


    <navigation-rule>
        <navigation-case>
            <from-outcome>admin.dashboard</from-outcome>
            <to-view-id>/admin/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>admin.users.edit</from-outcome>
            <to-view-id>/admin/users/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>admin.users.list</from-outcome>
            <to-view-id>/admin/users/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>search.results</from-outcome>
            <to-view-id>/search/search.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>curate.experiment</from-outcome>
            <to-view-id>/curate/experiment.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
