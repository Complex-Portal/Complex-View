<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 	"http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>IntAct: Exporter</display-name>
  
	<servlet>
		<servlet-name>Servlet</servlet-name>
		<display-name>Servlet</display-name>
		<servlet-class>uk.ac.ebi.intact.service.exporterWebApp.Servlet</servlet-class>
		<load-on-startup>100</load-on-startup>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>Servlet</servlet-name>
  		<url-pattern>/Servlet</url-pattern>
  	</servlet-mapping>
  	
	<servlet>
		<servlet-name>dynamicImage</servlet-name>
		<servlet-class>uk.ac.ebi.intact.service.exporterWebApp.ImageServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>dynamicImage</servlet-name>
		<url-pattern>/image</url-pattern>
	</servlet-mapping>
	
</web-app>
